{
    "name": "dpeuscher/alfred-symfony-tools",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "symfony/framework-bundle": "^4.0",
        "joetannenbaum/alfred-workflow": "^0.1.1",
        "symfony/console": "^4.1",
        "monolog/monolog": "^1.23",
        "makbari/dot-env-editor": "^0.1.4",
        "symfony/yaml": "^4.1",
        "symfony/dotenv": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Dpeuscher\\AlfredSymfonyTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dpeuscher\\AlfredSymfonyTools\\Tests\\": "tests/"
        }
    }
}
